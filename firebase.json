{
  "hosting": {
    "public": ".",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "rewrites": [
      {
        "source": "/api/**",
        "function": "gptChat"
      },
      {
        "source": "/music/**",
        "function": "musicGen"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
